{
  "manifest_version": 3,
  "name": "内容入れ替え",
  "description": "説明いらん",
  "version": "1.0",
  "action": {
    "default_popup": "表示.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://jpdb.io/*"
      ],
      "js": ["実行.js"],
      "run_at": "document_start",
      "all_frames": true,
      "match_about_blank": true
    }
  ]
}